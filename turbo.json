{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "outputs": [".next/**", "!.next/cache/**", "output"],
      "persistent": true
    },
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "output"]
    },
    "start": {
      "persistent": true,
      "outputs": [".next/**", "!.next/cache/**", "output"]
    },
    "typecheck": {},
    "typegen": {}
  }
}