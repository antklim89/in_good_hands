{
  "name": "@in-good-hands/server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "./src/server.ts",
  "useWorkspaces": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:**",
    "dev:esbuild": "vite-node -w ./src/server.ts",
    "dev:typecheck": "yarn typecheck -w --preserveWatchOutput",
    "build": "run-s build:**",
    "build:generate": "prisma generate",
    "build:migrate": "prisma migrate deploy",
    "build:generatedata": "vite-node ./src/generate-data.ts",
    "build:esbuild": "node esbuild.js",
    "start": "node ./output/server.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "migrate": "run-s migrate:**",
    "migrate:dev": "prisma migrate dev",
    "migrate:test": "DATABASE_URL=\"file:./data/test.sqlite\" prisma migrate deploy",
    "studio": "prisma studio --browser none"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@fastify/auth": "^4.4.0",
    "@fastify/compress": "^6.5.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.0.1",
    "@in-good-hands/share": "*",
    "@prisma/client": "^5.8.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.11",
    "fastify": "^4.25.2",
    "fastify-cli": "^6.0.1",
    "fastify-plugin": "^4.5.1",
    "form-auto-content": "^3.2.1",
    "fs-extra": "^11.2.0",
    "jimp": "^0.22.10",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.8.0",
    "sharp": "^0.33.2",
    "vite": "^5.0.11",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/sharp": "^0.31.1",
    "esbuild-node-externals": "^1.12.0",
    "npm-run-all": "^4.1.5",
    "tap": "^18.6.1",
    "typescript": "^5.3.3",
    "vite-node": "^1.2.0",
    "vitest": "^1.2.0"
  }
}
