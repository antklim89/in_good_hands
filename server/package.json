{
  "name": "@in-good-hands/server",
  "version": "1.0.0",
  "description": "Fastify server.",
  "main": "./src/server.ts",
  "useWorkspaces": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:**",
    "dev:vite-node": "vite-node -w ./src/server.ts",
    "dev:typecheck": "yarn typecheck -w --preserveWatchOutput",
    "build": "run-s build:**",
    "build:generateprisma": "prisma generate",
    "build:migrate": "prisma migrate deploy",
    "generatedata": "vite-node ./src/generate-data.ts",
    "start": "vite-node ./src/server.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio --browser none"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@fastify/auth": "^4.6.1",
    "@fastify/compress": "^7.0.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/static": "^7.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@in-good-hands/share": "*",
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-cli": "^6.1.1",
    "fastify-plugin": "^4.5.1",
    "fs-extra": "^11.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pino-pretty": "^11.0.0",
    "prisma": "^5.11.0",
    "sharp": "^0.33.2",
    "testcontainers": "^10.7.2",
    "vite": "^5.2.3",
    "vite-node": "^1.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/sharp": "^0.31.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  }
