{
  "name": "@in-good-hands/server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "./src/server.ts",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "scripts": {
    "dev": "run-p dev:**",
    "dev:nodemon": "nodemon ./output/server.js --ignore swagger/",
    "dev:swc": "swc ./src -d ./output -w",
    "dev:typecheck": "tsc --noEmit -w --preserveWatchOutput",
    "build": "run-s build:**",
    "build:swr": "swc ./src -d ./output",
    "build:migrate": "prisma migrate deploy",
    "build:generate": "swc ./src/generate-data.ts -d ./output && node ./output/generate-data.js",
    "start": "node ./output/server.js",
    "test": "jest --watchAll -i",
    "migrate": "run-s migrate:**",
    "migrate:dev": "prisma migrate dev",
    "migrate:test": "DATABASE_URL=\"postgresql://postgres@localhost:5432/test?schema=public\" prisma migrate deploy",
    "studio": "prisma studio --browser none"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fastify/auth": "^4.1.0",
    "@fastify/autoload": "^5.5.0",
    "@fastify/compress": "^6.1.1",
    "@fastify/cors": "^8.2.0",
    "@fastify/multipart": "^7.3.0",
    "@fastify/sensible": "^5.1.1",
    "@fastify/static": "^6.5.1",
    "@fastify/swagger": "^8.1.0",
    "@prisma/client": "^4.6.1",
    "@swc-node/register": "^1.5.4",
    "bcryptjs": "^2.4.3",
    "fastify": "^4.10.2",
    "fastify-cli": "^5.6.0",
    "fastify-plugin": "^4.3.0",
    "form-auto-content": "^3.0.0",
    "fs-extra": "^10.1.0",
    "jimp": "^0.16.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "prisma": "^4.6.1",
    "sharp": "^0.31.3",
    "swagger-typescript-api": "^12.0.2",
    "swc-loader": "^0.2.3",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.19",
    "@swc/jest": "^0.2.23",
    "@types/fs-extra": "^9.0.13",
    "@types/sharp": "^0.31.1",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "tap": "^16.3.2",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "typescript": "4.7.4"
  }
}
