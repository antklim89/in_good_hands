{
  "name": "@in-good-hands/server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "./src/server.ts",
  "useWorkspaces": true,
  "scripts": {
    "dev": "run-p dev:**",
    "dev:nodemon": "nodemon ./output/server.js --ignore swagger/",
    "dev:swc": "swc ./src -d ./output -w",
    "dev:typecheck": "yarn typecheck -w --preserveWatchOutput",
    "build": "run-s build:**",
    "build:generate": "prisma generate",
    "build:migrate": "prisma migrate deploy",
    "build:generatedata": "swc ./src/generate-data.ts -d ./output && node ./output/generate-data.js",
    "build:swr": "swc ./src -d ./output",
    "start": "node ./output/server.js",
    "typecheck": "tsc --noEmit",
    "test": "jest --watchAll -i",
    "migrate": "run-s migrate:**",
    "migrate:dev": "prisma migrate dev",
    "migrate:test": "DATABASE_URL=\"file:./data/test.sqlite\" prisma migrate deploy",
    "studio": "prisma studio --browser none"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@fastify/auth": "^4.3.0",
    "@fastify/autoload": "^5.7.1",
    "@fastify/compress": "^6.4.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/multipart": "^7.7.1",
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.10.2",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.0.1",
    "@prisma/client": "^5.0.0",
    "@swc-node/register": "^1.6.6",
    "bcryptjs": "^2.4.3",
    "fastify": "^4.20.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.1",
    "form-auto-content": "^3.1.0",
    "fs-extra": "^11.1.1",
    "jimp": "^0.22.8",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.0.1",
    "prisma": "^5.0.0",
    "sharp": "^0.32.3",
    "swc-loader": "^0.2.3",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "@swc/jest": "^0.2.26",
    "@types/fs-extra": "^11.0.1",
    "@types/sharp": "^0.31.1",
    "jest": "^29.6.1",
    "npm-run-all": "^4.1.5",
    "tap": "^16.3.7",
    "typescript": "^5.1.6"
  }
}
