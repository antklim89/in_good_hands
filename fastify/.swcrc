{
    "jsc": {
        "externalHelpers": false,
        "loose": true,
        "target": "es2020",
        "parser": {
            "syntax": "typescript",
            "dynamicImport": true
        },
        "baseUrl": ".",
        "paths": {
            "~/fastify/*": [
                "./src/*"
            ],
            "~/fastify/": [
                "./src/"
            ]
        }
    },
    "module": {
        "type": "commonjs",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    }
}