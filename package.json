{
  "name": "in-good-hands",
  "description": "In good hands",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "run-p dev:**",
    "dev:client": "yarn --cwd client dev",
    "dev:server": "yarn --cwd server dev",
    "build": "run-p build:**",
    "build:client": "yarn --cwd client build",
    "build:server": "yarn --cwd server build",
    "start": "run-p start:**",
    "start:client": "yarn --cwd client start",
    "start:server": "yarn --cwd server start",
    "test": "yarn --cwd server test",
    "migrate": "yarn --cwd server migrate"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.25.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "@types/cookie": "^0.5.1",
    "@types/node": "17.0.36",
    "@types/react": "18.0.9",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "npm-run-all": "^4.1.5",
    "typescript": "4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antklim89/in_good_hands.git"
  },
  "bugs": {
    "url": "https://github.com/antklim89/in_good_hands.git/issues"
  }
}